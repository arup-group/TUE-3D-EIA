[
  {
    "id": "gl-draw-line",
    "type": "line",
    "filter": [
      "all",
      [
        "==",
        "$type",
        "LineString"
      ],
      [
        "!=",
        "mode",
        "static"
      ]
    ],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "#4BB47C",
      "line-dasharray": [
        0.2,
        2
      ],
      "line-width": 4
    }
  },
  {
    "id": "gl-draw-polygon-fill",
    "type": "fill",
    "filter": [
      "all",
      [
        "==",
        "$type",
        "Polygon"
      ],
      [
        "!=",
        "mode",
        "static"
      ]
    ],
    "paint": {
      "fill-color": "#4deb09",
      "fill-outline-color": "#4BB47C",
      "fill-opacity": 0.6
    }
  },
  {
    "id": "gl-draw-polygon-stroke-active",
    "type": "line",
    "filter": [
      "all",
      [
        "==",
        "$type",
        "Polygon"
      ],
      [
        "!=",
        "mode",
        "static"
      ]
    ],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "#4BB47C",
      "line-dasharray": [
        0.2,
        2
      ],
      "line-width": 4
    }
  },
  {
    "id": "gl-draw-polygon-and-line-vertex-halo-active",
    "type": "circle",
    "filter": [
      "all",
      [
        "==",
        "meta",
        "vertex"
      ],
      [
        "==",
        "$type",
        "Point"
      ],
      [
        "==",
        "active",
        "true"
      ],
      [
        "!=",
        "mode",
        "static"
      ]
    ],
    "paint": {
      "circle-radius": 10,
      "circle-color": "#FFF"
    }
  },
  {
    "id": "gl-draw-polygon-and-line-vertex-active",
    "type": "circle",
    "filter": [
      "all",
      [
        "==",
        "meta",
        "vertex"
      ],
      [
        "==",
        "$type",
        "Point"
      ],
      [
        "!=",
        "mode",
        "static"
      ]
    ],
    "paint": {
      "circle-radius": 5,
      "circle-color": "#4BB47C"
    }
  },
  {
    "id": "gl-draw-point",
    "type": "circle",
    "filter": [
      "all",
      [
        "==",
        "meta",
        "midpoint"
      ],
      [
        "==",
        "$type",
        "Point"
      ]
    ],
    "paint": {
      "circle-radius": 4,
      "circle-stroke-color": "#FFFFFF",
      "circle-color": "#B2EACB",
      "circle-stroke-width": 3
    }
  },
  {
    "id": "gl-draw-line-inactive",
    "type": "line",
    "filter": [
      "all",
      [
        "==",
        "$type",
        "LineString"
      ],
      [
        "==",
        "mode",
        "static"
      ]
    ],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "#000",
      "line-width": 113
    }
  },
  {
    "id": "gl-draw-polygon-fill-inactive",
    "type": "fill",
    "filter": [
      "all",
      [
        "==",
        "$type",
        "Polygon"
      ],
      [
        "==",
        "mode",
        "static"
      ]
    ],
    "paint": {
      "fill-color": "#000",
      "fill-outline-color": "#000",
      "fill-opacity": 0.1
    }
  },
  {
    "id": "gl-draw-polygon-stroke-inactive",
    "type": "line",
    "filter": [
      "all",
      [
        "==",
        "$type",
        "Polygon"
      ],
      [
        "==",
        "mode",
        "static"
      ]
    ],
    "layout": {
      "line-cap": "round",
      "line-join": "round"
    },
    "paint": {
      "line-color": "#000",
      "line-width": 131
    }
  }
]